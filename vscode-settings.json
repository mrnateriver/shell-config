{
  "editor.fontLigatures": true,
  "editor.fontFamily": "'JetBrainsMono Nerd Font', Consolas, 'Courier New', monospace",
  "editor.fontWeight": "normal",
  "editor.lineHeight": 1.8,
  "terminal.integrated.fontFamily": "'JetBrainsMono Nerd Font'",
  "todo-tree.highlights.enabled": false,
  "redhat.telemetry.enabled": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnSave": false
  },
  "git.autofetch": true,
  "editor.lineNumbers": "relative",
  "vim.easymotion": true,
  "vim.useSystemClipboard": true,
  "vim.surround": true,
  "vim.leader": " ",
  "vim.foldfix": true,
  "vim.camelCaseMotion.enable": true,
  "vim.matchpairs": "(:),{:},[:],<:>",
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-s>": false,
    "<C-z>": true,
    "<C-w>": false,
    "<C-q>": false,
    "<C-i>": false,
    "<C-o>": false
  },
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<leader>",
        "s",
        "f"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<leader>",
        "d",
        "s"
      ],
      "commands": [
        "workbench.action.gotoSymbol"
      ]
    },
    {
      "before": [
        "<leader>",
        "w",
        "s"
      ],
      "commands": [
        "workbench.action.showAllSymbols"
      ]
    },
    {
      "before": [
        "<leader>",
        "f"
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "n"
      ],
      "commands": [
        "editor.action.rename"
      ]
    },
    {
      "before": [
        "<leader>",
        "c",
        "a"
      ],
      "commands": [
        "editor.action.quickFix"
      ]
    },
    {
      "before": [
        "g",
        "d"
      ],
      "commands": [
        "editor.action.openLink",
        "editor.action.revealDefinition"
      ]
    },
    {
      "before": [
        "<leader>",
        "a"
      ],
      "commands": [
        "outline.focus"
      ]
    },
    {
      "before": [
        "<leader>",
        "q"
      ],
      "commands": [
        "workbench.actions.view.problems"
      ]
    },
    {
      "before": [
        "%"
      ],
      "commands": [
        "editor.action.jumpToBracket"
      ]
    },
    {
      "before": [
        "\\"
      ],
      "commands": [
        "workbench.files.action.showActiveFileInExplorer"
      ]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "<C-i>"
      ],
      "commands": [
        "inlineChat.start"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    },
    {
      "before": [
        "%"
      ],
      "commands": [
        "editor.action.jumpToBracket"
      ]
    }
  ],
  "editor.fontSize": 14,
  "settingsSync.keybindingsPerPlatform": false,
  "background.renderContentAboveBackground": true,
  "background.smoothImageRendering": true,
  "background.windowBackgrounds": [
    "https://948749-mrnateriver.tmweb.ru:23891/bg.jpg"
  ],
  "background.backgroundBlur": [
    "0px",
    "0",
    "0",
    "0"
  ],
  "editor.minimap.enabled": false,
  "git.confirmSync": false,
  "background.autoInstall": false,
  "notebook.outline.showCodeCells": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 5000,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
    // "editor.formatOnSave": true
  },
  "editor.linkedEditing": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
    // "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "tailwind-fold.autoFold": false,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "background.backgroundOpacity": [
    0.95,
    0.9,
    0.9,
    0.9
  ],
  // "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "typescript.tsserver.web.typeAcquisition.enabled": true,
  "typescript.experimental.updateImportsOnPaste": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.implementationsCodeLens.enabled": true,
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.tsserver.log": "off",
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.documentSelectors": [
    "*.json"
  ],
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  "workbench.colorTheme": "GitHub Dark Default",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "background.useInvertedOpacity": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "terminal.integrated.allowedLinkSchemes": [
    "file",
    "http",
    "https",
    "mailto",
    "vscode",
    "vscode-insiders",
    "docker-desktop"
  ],
  "playwright.showTrace": false,
  "Prettier-SQL.SQLFlavourOverride": "postgresql",
  "editor.inlayHints.enabled": "offUnlessPressed",
  "git.suggestSmartCommit": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "diffEditor.maxComputationTime": 0,
  "diffEditor.renderSideBySide": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.blame.editorDecoration.enabled": true,
  "stylelint.config": {},
  "stylelint.validate": [
    "css",
    "postcss",
    "scss",
    "sass"
  ],
  "editor.codeActionsOnSave": {},
  "terminal.integrated.minimumContrastRatio": 1,
  "chat.agent.enabled": true,
  "github.copilot.chat.agent.thinkingTool": true,
  "workbench.editorAssociations": {
    "*.copilotmd": "vscode.markdown.preview.editor",
    "{hexdiff}:/**/*.*": "hexEditor.hexedit",
    "file:/**/*.csv": "jupyter-data-wrangler"
  },
  "notebook.lineNumbers": "on",
  "docker.extension.enableComposeLanguageServer": false,
  "github.copilot.chat.codesearch.enabled": true,
  "chat.agent.maxRequests": 128,
  "chat.tools.autoApprove": true,
  "csharp.inlayHints.enableInlayHintsForTypes": true,
  "csharp.inlayHints.enableInlayHintsForLambdaParameterTypes": true,
  "editor.stickyScroll.enabled": true,
  "remote.defaultExtensionsIfInstalledLocally": [
    "GitHub.copilot",
    "GitHub.copilot-chat",
    "GitHub.vscode-pull-request-github"
  ],
  "plantuml.render": "Local"
}
